#include <stdio.h>
#include <stdlib.h>
int main()
{
    char a[1000];
    FILE *fptr;
    if ((fptr = fopen("program.txt", "r")) == NULL)
    {
        printf("!!Error!! opening file");
        exit(1)      
    }
   
    scanf(fptr, %[^\n]", a)
    printf("Data files:\n%s", a);
    close(fptr)
    
    return 0;
}
